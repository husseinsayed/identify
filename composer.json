    "keywords": ["github", "laravel","Open Source","Evangelist", "Detect", "Identify", "Browser", "Operating System", "Language"],
    "license": "MIT",
    "authors": [
      {
        "name": "husseinsayed",
        "email": "eng.husseinsayed@gmail.com"
      }
   ],
   "minimum-stability": "stable",
   "require": {
    "php": "*",
    "illuminate/support": "*",
    "sinergi/browser-detector": "*"
   },
   "autoload": {
        "psr-4": {
          "HusseinSayed\\Identify\\": "src/"
        }
   },
   "autoload-dev": {
